{
  "name": "angular1-webpack-typescript",
  "version": "0.1.2",
  "description": "A seed project for a largescale angular architecture in webpack and typescript",
  "main": "/",
  "scripts": {
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "start": "./node_modules/.bin/webpack --config webpack/webpack.dev.js --watch --NODE_ENV=dev",
    "test": "karma start --NODE_ENV=test",
    "build": "./node_modules/.bin/webpack --config webpack/webpack.build.js --NODE_ENV=production",
    "postinstall": "./node_modules/.bin/typings i"
  },
  "repository": "angular1-webpack-typescript",
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.8",
    "angular-ui-bootstrap": "2.1.4",
    "moment": "2.15.1",
    "ramda": "0.22.1"
  },
  "devDependencies": {
    "angular-mocks": "1.5.8",
    "babel-core": "6.17.0",
    "babel-istanbul": "0.11.0",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "firebase-tools": "~3.0.8",
    "html-webpack-plugin": "2.22.0",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "1.0.0",
    "jasmine": "2.5.2",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor": "0.3.0",
    "karma-webpack": "1.8.0",
    "node-sass": "3.10.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.12",
    "raw-loader": "0.5.1",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "wallaby-webpack": "0.0.25",
    "webpack": "1.13.2"
  }
}
